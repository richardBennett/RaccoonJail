{
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      },
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "c:\\Logs\\RaccoonJailApi\\log.json",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      },
      { "Name": "Console" }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "#{database.connection_string.dashboards}#",
    "CareConnection": "#{database.connection_string.care}#"
  }
}